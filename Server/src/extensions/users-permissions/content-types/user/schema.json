{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phoneNumber": {
      "type": "biginteger"
    },
    "favoriteFilms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::film-s.film-s"
    },
    "favoriteSeries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::series-s.series-s"
    },
    "cardName": {
      "type": "string"
    },
    "cardSname": {
      "type": "string"
    },
    "cardNumber": {
      "type": "string"
    },
    "securityCode": {
      "type": "integer",
      "min": 100
    },
    "expiryDate": {
      "type": "string",
      "minLength": 5
    },
    "paymentsOffer": {
      "type": "boolean"
    },
    "paymentsProcessing": {
      "type": "enumeration",
      "enum": [
        "creditCard",
        "payPal"
      ],
      "default": "creditCard"
    },
    "optInSubscription": {
      "type": "boolean"
    },
    "payment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment.payment",
      "inversedBy": "user"
    }
  }
}
